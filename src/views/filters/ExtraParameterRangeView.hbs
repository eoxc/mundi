<label class="col-sm-6 control-label"
  {{#if title}}
    data-toggle="tooltip" data-placement="top" title="{{t title}}"
  {{/if}}
>{{t displayName}}</label>
<div class="col-sm-6">
  <input value=""
    data-type="{{ type }}"
    data-provide="slider"
    data-slider-min="{{ min }}"
    data-slider-max="{{ max }}"
    data-slider-step="{{ step }}"
    data-slider-value="[{{ low }},{{ high }}]"
    data-slider-tooltip=""
    />
</div>
